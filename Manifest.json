{
    "manifest_version": 3,
    "name": "Proxy Switcher (Click Icon)",
    "version": "1.1",
    "description": "Enables/disables a SOCKS5 proxy by clicking the icon",
    "action": {
        "default_title": "Proxy Switcher",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "permissions": ["activeTab", "proxy", "storage"],
    "background": {
      "service_worker": "background.js"
    }
  }
  